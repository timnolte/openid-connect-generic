		"wp-content/plugins/debug-bar": "./vendor/wpackagist-plugin/debug-bar",
		"wp-content/plugins/query-monitor": "./vendor/wpackagist-plugin/query-monitor",
		"wp-content/plugins/wp-debugging": "./vendor/wpackagist-plugin/wp-debugging"
	},
	"config": {
